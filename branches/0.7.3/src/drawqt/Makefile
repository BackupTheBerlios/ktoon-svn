dist: 
	@mkdir -p .tmp/drawqt && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/drawqt/ && ( cd `dirname .tmp/drawqt` && $(TAR) drawqt.tar drawqt && $(GZIP) drawqt.tar ) && $(MOVE) `dirname .tmp/drawqt`/drawqt.tar.gz . && $(DEL_FILE) -r .tmp/drawqt

