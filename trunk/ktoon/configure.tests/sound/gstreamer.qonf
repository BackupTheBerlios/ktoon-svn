<?xml version="1.0" ?>
<qonf>
	<test name="Gstreamer 0.10">
		<dir path="." />
	</test>
	<provide>
		<defines>
			<define value="HAVE_SOUND" />
		</defines>
		<includes>
			<command value="pkg-config gstreamer-0.10 --cflags --silence-errors" />
		</includes>
		<libs>
			<command value="pkg-config gstreamer-0.10 --libs --silence-errors" />
		</libs>
	</provide>
	<solution>
		<linux package="aspell" url="" comment="" >
			<distribution name="gentoo" package="aspell" url="" comment="" />
			<distribution name="ubuntu" package="aspell-dev" url="" comment="" />
		</linux>
		<windows package="aspell" url="" comment="" />
		<macosx package="aspell" url="" comment="" />
	</solution>
</qonf>

