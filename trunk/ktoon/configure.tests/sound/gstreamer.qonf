<?xml version="1.0" ?>
<qonf optional="true">
	<test name="Gstreamer 0.10">
		<dir path="." />
	</test>
	<provide>
		<defines>
			<define value="HAVE_SOUND" />
			<define value="HAVE_GST10" />
		</defines>
		<includes>
			<command value="pkg-config gstreamer-0.10 --cflags --silence-errors" />
		</includes>
		<libs>
			<command value="pkg-config gstreamer-0.10 --libs --silence-errors" />
		</libs>
	</provide>
	<solution>
		<linux package="gstreamer" url="http://gstreamer.sourceforge.net" comment="" >
			<distribution name="gentoo" package="gstreamer" url="http://gstreamer.sourceforge.net" comment="Try: emerge gstreamer" />
			<distribution name="ubuntu" package="libgstreamer0.10-dev" url="http://gstreamer.sourceforge.net" comment="Try: apt-get install libgstreamer0.10-dev" />
		</linux>
		<windows package="aspell" url="http://gstreamer.sourceforge.net" comment="FIXME" />
		<macosx package="aspell" url="http://gstreamer.sourceforge.net" comment="FIXME" />
	</solution>
</qonf>

