	</test>
	<provide>
		<defines>
			<define value="HAVE_FFMPEG" />
		</defines>
		<libs>
			<lib path="-lavcodec" />
			<lib path="-lavformat" />
		</libs>
	</provide>
	<solution>
		<linux package="ffmpeg" url="" comment="" >
			<distribution name="gentoo" package="ffmpeg" url="" comment="" />
			<distribution name="ubuntu" package="ffmpeg-dev" url="" comment="" />
		</linux>
		<windows package="ffmpeg" url="" comment="" />
		<macosx package="ffmpeg" url="" comment="" />
	</solution>
</qonf>
