#############################################################################
# Makefile for building: ktoon
# Generated by qmake (1.07a) (Qt 3.3.3) on: Tue Apr 26 11:11:35 2005
# Project:  ktoon.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile ktoon.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-ming \
		sub-src

first: all

all: Makefile $(SUBTARGETS)

ming/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "ming" || $(MKDIR) "ming"
	cd ming && $(QMAKE) ming.pro -o $(MAKEFILE)
sub-ming: ming/$(MAKEFILE) FORCE
	cd ming && $(MAKE) -f $(MAKEFILE)

src/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src" || $(MKDIR) "src"
	cd src && $(QMAKE) src.pro -o $(MAKEFILE)
sub-src: src/$(MAKEFILE) FORCE
	cd src && $(MAKE) -f $(MAKEFILE)

Makefile: ktoon.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile ktoon.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile ktoon.pro

all: $(SUBTARGETS)
qmake_all: ming/$(MAKEFILE) src/$(MAKEFILE)
	 ( [ -d ming ] && cd ming ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d src ] && cd src ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d ming ] && cd ming ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d ming ] && cd ming ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d ming ] && cd ming ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d ming ] && cd ming ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

