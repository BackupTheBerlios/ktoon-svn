$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: cameras.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile cameras.pro
qmake: 
	@$(QMAKE) -o Makefile cameras.pro

dist: 
	@mkdir -p .tmp/cameras && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/cameras/ && ( cd `dirname .tmp/cameras` && $(TAR) cameras.tar cameras && $(GZIP) cameras.tar ) && $(MOVE) `dirname .tmp/cameras`/cameras.tar.gz . && $(DEL_FILE) -r .tmp/cameras

