dist: 
	@mkdir -p .tmp/color && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/color/ && ( cd `dirname .tmp/color` && $(TAR) color.tar color && $(GZIP) color.tar ) && $(MOVE) `dirname .tmp/color`/color.tar.gz . && $(DEL_FILE) -r .tmp/color

