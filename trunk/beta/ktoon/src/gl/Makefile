qmake: 
	@$(QMAKE) -o Makefile gl.pro

dist: 
	@mkdir -p .tmp/gl && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/gl/ && ( cd `dirname .tmp/gl` && $(TAR) gl.tar gl && $(GZIP) gl.tar ) && $(MOVE) `dirname .tmp/gl`/gl.tar.gz . && $(DEL_FILE) -r .tmp/gl

